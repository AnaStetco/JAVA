class ar
{
double bredth,length;

  
   void getval()
    {
      bredth=20;
      length=10;


    }
   double area()
   {
     double totalarea;
     totalarea=bredth*length;
     return totalarea;
   }
  void display()
  {
    System.out.println("this is bredth of rectangle: "+bredth);
    System.out.println("this is bredth of rectangle: "+length);


  }
}
