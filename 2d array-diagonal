   for(j=0;j<4;j++)
    {

    
     
      if(i+j==4-1)
       {
          System.out.print(" " +a1[i][j]);
          //sum=sum+a1[i][j];
          // System.out.println(" "+sum);
     /*  }   
      else
      {
       
      System.out.print("  ");*/
       } 
   }
    
    //System.out.println("");// creating a new line

   }// end of i





  


  }

}





   
   
