class mod
{
public static void main(String args[])
{
int a[]={5,6,32,9,45};

int i,sum1,sum2;
sum1=0;
sum2=0;

for(i=0;i<5;i=i+1)
  {
    if(a[i]%2==0)
     {
      sum1=sum1+a[i];
     }
    
     if(a[i]%2!=0)
     
       {
	sum2=sum2+a[i];
        }

   }
  System.out.println("this is summation of even number:  "+sum1);
  System.out.println("this is summatio of odd numbers: " +sum2);

  }
}
